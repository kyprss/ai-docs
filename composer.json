{
    "name": "kyprss/ai-docs",
    "description": "An easy tool to download the latest documentation from a git repository",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "symfony/console": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Kyprss\\AiDocs\\": "src/"
        }
    },
    "bin": [
        "bin/ai-docs"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
